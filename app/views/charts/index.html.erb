<h1>Spline Chart</h1>

<div id="container">

</div>

<script type="text/javascript">
  $(document).ready(function() {

    var options = {
        chart: {
            renderTo: 'container',
            type: 'spline'
        },
        series: [{}]
    };

    // Ajax call to HTTP GET method from RESTful service
    $.getJSON('/api/v1/charts', function(data) {
        options.series[0].data = data;
        var chart = new Highcharts.Chart(options);
    });

  });
</script>
